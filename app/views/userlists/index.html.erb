<h1><%= User.model_name.human %></h1>

<table>
  <thead>
    <tr>
      <th><%= User.human_attribute_name(:email) %></th>
      <th>住所</th>
      <th>自己紹介</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.email %></td>
        <td><%= book.address %></td>
        <td><%= book.selfintroduction %></td>
        <td><%= link_to t('views.common.edit'), edit_user_registration_path(book) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @books %>

<br>

<%= link_to t('views.common.back'), books_path %>
